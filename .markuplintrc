{
  "rules": {
    "attr-duplication": true,
    "deprecated-attr": true,
    "deprecated-element": true,
    "disallowed-element": [],
    "doctype": "always",
    "heading-levels": false,
    "id-duplication": true,
    "invalid-attr": true,
    "no-duplicate-dt": true,
    "no-empty-palpable-content": false,
    "no-orphaned-end-tag": true,
    "permitted-contents": [],
    "placeholder-label-option": false,
    "require-datetime": true,
    "required-attr": false,
    "required-element": [],

    "label-has-control": false,
    "landmark-roles": false,
    "neighbor-popovers": true,
    "no-consecutive-br": false,
    "no-refer-to-non-existent-id": true,
    "require-accessible-name": false,
    "required-h1": true,
    "use-list": false,
    "wai-aria": false,

    "class-naming": false,

    "no-hard-code-id": false,
    "no-use-event-handler-attr": false,

    "attr-value-quotes": true,
    "case-sensitive-attr-name": true,
    "case-sensitive-tag-name": true,
    "character-reference": false,
    "end-tag": true,
    "ineffective-attr": false,
    "no-boolean-attr-value": false,
    "no-default-value": false
  },
  "nodeRules": [
    {
      "selector": "meta[property]",
      "rules": {
        "invalid-attr": {
          "options": {
            "allowAttrs": ["property", "content"]
          }
        }
      }
    }
  ],
  "excludeFiles": ["./out/contact/index.html"]
}
