{
  "ci": {
    "collect": {
      "staticDistDir": "./out",
			"numberOfRuns": 1,
			"staticDirFileDiscoveryDepth": 3,
      "settings": {
        "locale": "ja"
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    },
    "assert": {
			"preset": "lighthouse:recommended",
      "assertions": {
        "categories:performance": [
					"warn", {
						"median": 0.89
					}
				]
      }
    }
  }
}
