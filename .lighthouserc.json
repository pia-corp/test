{
  "ci": {
    "collect": {
			"staticDistDir": "./out/",
      "autodiscoverUrlBlocklist": "./out/news/page/",
			"numberOfRuns": 3,
			"maxAutodiscoverUrls": 5,
      "settings": {
        "locale": "ja"
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    },
    "assert": {
      "assertions": {
        "first-contentful-paint": ["warn", { "minScore": 0.8 }]
      }
    }
  }
}
