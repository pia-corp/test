{
  "ci": {
    "collect": {
      "staticDistDir": "./out",
			"numberOfRuns": 1,
			"staticDirFileDiscoveryDepth": 3,
      "settings": {
        "locale": "ja"
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    },
    "assert": {
      "assertions": {
        "first-contentful-paint": ["error", { "minScore": 0.8 }]
      }
    }
  }
}
