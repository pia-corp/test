{
  "ci": {
    "collect": {
			"staticDistDir": "./public/",
			"numberOfRuns": 1,
      "settings": {
        "locale": "ja"
      }
    },
    "budgetsFile": {
      "budgetsFile": "./.github/config/budget.json"
    },
    "upload": {
      "target": "temporary-public-storage"
    },
    "assert": {
      "assertions": {
        "performance-budget": "error"
      }
    }
  }
}
